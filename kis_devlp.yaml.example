# 한국투자증권 API 설정 파일
# kis_devlp.yaml로 복사하여 사용하세요

# 모의투자 (vps) 설정
appkey_vps: "여기에_모의투자_APP_KEY_입력"
appsecret_vps: "여기에_모의투자_APP_SECRET_입력"

# 실전투자 (prod) 설정
appkey_prod: "여기에_실전투자_APP_KEY_입력"
appsecret_prod: "여기에_실전투자_APP_SECRET_입력"
account_prod: "12345678-01"  # 계좌번호 (8자리-2자리)

# 사용 방법:
# 1. config.json의 kis.server를 "vps" 또는 "prod"로 설정
# 2. vps: 모의투자 / prod: 실전투자
